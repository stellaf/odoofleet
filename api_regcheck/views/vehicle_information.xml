<odoo>
  <data>
    <record model="ir.ui.view" id="view_vehicle_information_tree">
      <field name="name">vehicle.information.tree</field>
      <field name="model">vehicle.information</field>
      <field name="type">tree</field>
      <field name="arch" type="xml">
        <tree>
          <field name="registration_number" string="Registration Number"/>
          <field name="make" string="Car brand"/>
          <field name="model" string="Model"/>
          <field name="color" string="Color"/>
          <field name="year" string="Year"/>
          <!-- <field name="EngineSize" string="Horsepower"/> -->
          <field name="BodyStyle" string="car model "/>
          <field name="NumberOfSeats" string="Seats"/>
          <field name="RegistrationDate" string="RegistrationDate"/>
          <field name="FuelType" string="FuelType"/>
          <field name="IndicativeValue" string="varde"/>

        </tree>
      </field>
    </record>
  </data>
</odoo>
